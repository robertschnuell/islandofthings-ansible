{
  "title": "{{ etherpad_title }}",
  "favicon": "src/static/favicon.ico",
  "skinName": "medienhaus",
  "ip": "10.0.0.3",
  "port": 9001,
  "dbType" : "postgres",
  "dbSettings" : {
    "user": "{{ etherpad_database_user }}",
    "host": "{{ etherpad_database_host }}",
    "password": "{{ etherpad_database_password }}",
    "database": "{{ etherpad_database_database }}",
    "charset": "utf8"
  },
  "defaultPadText" : "",
  "padOptions": {
    "noColors": true,
    "showControls": true,
    "showChat": false,
    "showLineNumbers": true,
    "useMonospaceFont": true,
    "userName": false,
    "userColor": true,
    "rtl": false,
    "alwaysShowChat": false,
    "chatAndUsers": false,
    "lang": "en-us"
  },
  "users": {
    "{{ etherpad_admin_user }}": {
      "password": "{{ etherpad_admin_password }}",
      "is_admin": true
    }
  }
}
